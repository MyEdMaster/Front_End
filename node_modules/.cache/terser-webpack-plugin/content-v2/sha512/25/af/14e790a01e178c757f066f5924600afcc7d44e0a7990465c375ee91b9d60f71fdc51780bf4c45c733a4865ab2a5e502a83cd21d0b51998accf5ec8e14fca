 4 rules.\"),s.a.createElement(\"br\",null),s.a.createElement(p.u,{className:oe.a.border},s.a.createElement(\"p\",{className:oe.a.ph2},\"For example, \\xa0\",s.a.createElement(ye.InlineMath,null,\"i^9 = 1\"),\" \\xa0, because \\xa0\",s.a.createElement(ye.InlineMath,null,\"i^9 = i(i^4)^2 = i(1^2) = i\"),\" \\xa0.\",s.a.createElement(\"tr\",null),s.a.createElement(\"br\",null),\"Another example is that \\xa0\",s.a.createElement(ye.InlineMath,null,\"i^23 = -i\"),\" \\xa0, because \\xa0\",s.a.createElement(ye.InlineMath,null,\"i^23 = i^3(i^4)^5 = i^3(1)^5 = -i\"),\" \\xa0. See? Easy!\")))),s.a.createElement(p.u,{center:!0},s.a.createElement(p.e,{size:\"2\"},s.a.createElement(p.c,{color:\"deep-purple lighten-1\",className:\"\".concat(oe.a.btn,\" w-100 mt-3\"),onClick:function(){e.props.history.goBack()}},\"Previous Lesson\")),s.a.createElement(p.e,{size:\"2\"},s.a.createElement(p.c,{color:\"deep-purple lighten-1\",className:\"\".concat(oe.a.btn,\" w-100 mt-3\"),onClick:function(){e.props.history.push(\"/complex/add\")}},\"Next Lesson\"))))}}]),t}(s.a.Component),Se=function(e,t,a,n){var l=\"+\",s=\"+\",r=t,o=n;return\"-\"===t.substr(0,1)&&(l=\"-\",r=t.substr(1,1)),\"-\"===n.substr(0,1)&&(s=\"-\",o=n.substr(1,1)),[l,r,s,o]},Oe=function(e){function t(e){var a;return Object(i.a)(this,t),(a=Object(m.a)(this,Object(u.a)(t).call(this,e))).toggle=function(e){return function(){var t=\"modal\"+e;a.setState(Object(w.a)({},t,!a.state[t]))}},a.post=function(){var e={method:\"POST\",headers:{\"content-type\":\"application/json\"},body:JSON.stringify({A:a.state.a,B:a.state.b,C:a.state.c,D:a.state.d,step:a.state.step.toString(),answer:a.state.value})};fetch(\"\".concat(R,\"/complex_number/1\"),e).then(function(e){return e.json()}).then(function(e){if(a.setState({hint:e.content}),\"0\"===e.type)a.setState({isRight:!1});else if(\"1\"===e.type){var t=a.state.answers,n=e.step;t.push([n,a.state.value]),a.setState({answers:t,step:n,isRight:!0,value:\"\"})}else if(\"2\"===e.type){var l=a.state